    "formToIVRLinkage": {
      "description": "Mapping between Order Forms and their corresponding IVRs",
      "linkages": {
        "form1_ACZ": {
          "orderFormName": "ACZ Distribution Order Form",
          "correspondingIVR": "form1_ACZ",
          "ivrFormName": "ACZ Distribution IVR",
          "sharedIdentifiers": ["facilityName", "patientId", "procedureDate"],
          "notes": "Both forms share same vendor and product lineup"
        },
        "form4_BioWound": {
          "orderFormName": "BioWound Solutions Purchase Order",
          "correspondingIVR": "form4_BioWound",
          "ivrFormName": "BioWound Solutions IVR",
          "sharedIdentifiers": ["facilityName", "patientInfo"],
          "notes": "Order form follows IVR verification"
        },
        "form5_AdvancedSolution": {
          "orderFormName": "Advanced Solution Order Form",
          "correspondingIVR": "form5_AdvancedSolution",
