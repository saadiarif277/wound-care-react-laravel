    "implementationGuidelines": {
      "orderProcessingFlow": [
        "1. Complete Insurance Verification Request (IVR) form",
        "2. Receive insurance approval/verification",
        "3. Complete corresponding Order Form with product details",
        "4. Submit order form to appropriate email/fax",
        "5. Track order using PO# or Order Number"
      ],
      "dataValidation": {
        "requiredForAllOrders": [
          "Facility/shipping information",
          "Product selection with quantities",
          "Anticipated application/procedure date",
          "Contact information"
        ],
        "conditionalRequirements": [
          "Provider NPI (ExtremityCare forms)",
          "Billing information (if different from shipping)",
          "Purchase Order number (varies by vendor)"
        ]
      },
      "integrationPoints": {
        "ivrToOrder": "Patient and facility data should flow from IVR to Order",
        "orderToFulfillment": "Order data feeds into shipping/billing systems",
        "trackingUpdates": "Order numbers link to shipment tracking"
      }
